class Employee{
	private  int id;
	private String name;
	private double salary;
	
	//ctr
	public Employee() {
		id=121;
		name="raj";
		salary=56000;
	}
	 public void print() {
		 System.out.println("id : "+ id+ " name : "+ name+" salary: "+ salary);
	 }
	
}
public class E_EmployeeDemo {

	public static void main(String[] args) {
		Employee e=new Employee();// default ctr
		
		e.print();
		
	}
}
